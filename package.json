{
  "name": "123",
  "version": "0.1.0",
  "description": "A personal front page powered by Pagic",
  "scripts": {
    "start": "deno run --unstable --allow-read --allow-write --allow-net --allow-run https://deno.land/x/pagic/mod.ts build --serve --watch",
    "build": "deno run --unstable --allow-read --allow-write --allow-net --allow-run https://deno.land/x/pagic/mod.ts build",
    "install": "deno install --unstable --allow-read --allow-write --allow-net --allow-run --name=pagic https://deno.land/x/pagic/mod.ts
",
    "updatefavicon": "deno run --allow-net --allow-read --allow-write update-favicon.ts"
    "deploy:vercel": "sh deploy-vercel.sh"
  },
  "author": "izumiko <yosoro@outlook.com>",
  "license": "MIT"
}
